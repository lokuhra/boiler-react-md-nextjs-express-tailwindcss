    "next": "latest",
    "postcss-all-link-colors": "^2.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-grid-kiss": "^1.4.2",
    "postcss-responsive-font": "^1.0.3",
    "prettier": "^1.13.0",
    "react": "^16.4.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-icons": "^2.2.7",
    "react-md": "^1.3.2",
    "tailwindcss": "^0.5.3",
    "tailwindcss-blend-mode": "^0.1.0",
    "tailwindcss-cursor-extended": "^0.1.0"
